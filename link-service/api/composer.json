{
  "name": "shorty/link-service",
  "type": "project",
  "require": {
    "php": "^8.3",
    "yiisoft/yii2": "^2.0.46",
    "yiisoft/yii2-swiftmailer": "^2.1",
    "php-amqplib/php-amqplib": "^3.7",
    "predis/predis": "^2.0",
    "yidas/yii2-composer-bower-skip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "app\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "yidas/composer-bower-skip": true
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}